
Persona
	* Nombres			varchar 50
	* Apellidos			varchar 100
	* Fecha Nacimiento	Date
	* Documento			char 8
	* Altura			float
	* Sexo				enum (masculino, femenino,otros)
	* Celular			tinyint
	* Direccion			text
	* Telefono			tinyint

	
	
	
	create table personas (
	
		nombre varchar (50),
		apellidos varchar (100),
		fecha_nacimiento date,
		documento char (8),
		altura float (10.2),
		Sexo enum ('Masculino','Femenino', 'Otros'),
		celular tinyint (10),
		direcciones text,
		telefono tinyint (10)
	);
	
	SHOW TABLES;
	
	DROP TABLE personas;
    